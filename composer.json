{
  "name": "lunr/ticks.mqtt",
  "description": "Lunr.Ticks.Mqtt wrapper around php-mqtt/client that gathers analytics",
  "keywords": [ "analytics", "mqtt" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks.mqtt",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "lunr/ticks": "~0.12.0",
    "php-mqtt/client": "~2.2",
    "psr/log": ">=1.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=10.5 <11.0",
    "lunr/halo": "~0.11.0",
    "mockery/mockery": "~1.6",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
